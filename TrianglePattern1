

import java.util.Scanner;

public class TrianglePattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i=1;
       // int j=1;
        while(i<=n){
        	 int j=1;
        	 int startingno=i;
            while(j<=i){
                System.out.print(startingno);
                j++;
                startingno++;
            }
            System.out.println();
            i++;
            
        }
	}
}

output:

1
23
345
4567
