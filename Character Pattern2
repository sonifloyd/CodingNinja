

import java.util.Scanner;

public class Pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int i =1;
		int no=0;
		while(i<=n) {
			//reverseno=i;
			int j=1;
			//int no=i;
			while(j<=n) {
				System.out.print((char)('A'+j-1+no));
				j++;
				
				
			}
			System.out.println();
			i++;
			no++;
			
			
		}
		
		

	}

}


output:
4
ABCD
BCDE
CDEF
DEFG
